<div class="wrapped">
	<div class="show-form">
		<%= render 'form' %>
	</div>
	<div class="container">
		<div class="top">
			<div class="map">
				<%= image_tag @result.map %>
				<div class="clear"></div>
			</div>
			<div class="main-info">
				<div class="university-name">
					<span>
						<%= @result.name %>
					</span>
				</div>
				<div class="university-address">
					<span>
						<%= @result.address%>
					</span>
				</div>
			</div>	
			<div class="clear"></div>
		</div>
	</div>
	<div class="clear"></div>
</div>